{
    "name": "symfony-si/magento1-sl-si",
    "type": "magento-module",
    "license": "OSL-3.0",
    "description": "Slovenian (Slovenia) - sl_SI translations for Magento 1.x Community Edition",
    "homepage": "https://github.com/symfony-si/magento1-sl-si",
    "keywords": ["slovenian", "language", "sl_SI"],
    "authors": [
        {
            "name": "Peter Kokot",
            "email": "peterkokot@gmail.com"
        }
    ],
    "require": {
        "magento-hackathon/magento-composer-installer": "*"
    },
    "require-dev": {
        "php": ">=7.0.0",
        "phpunit/phpunit": "^6.2",
        "mikey179/vfsStream": "^1"
    },
    "autoload-dev": {
        "psr-4": {
            "SymfonySi\\LocalePack\\": "src/",
            "SymfonySi\\Test\\": "tests/"
        }
    },
    "extra": {
        "magento-root-dir": "root"
    }
}
